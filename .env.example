# ─── Google Maps / Places API ────────────────────────────────────────────────
# Required for real search results. Get from https://console.cloud.google.com
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# ─── Clerk Authentication ─────────────────────────────────────────────────────
# Get from https://dashboard.clerk.com → API Keys
# Publishable key is safe to expose to the browser (starts with pk_)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here
# Secret key is server-only (starts with sk_)
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key_here

# ─── Supabase Database ────────────────────────────────────────────────────────
# Get from https://supabase.com → Project Settings → API
SUPABASE_URL=https://your-project-id.supabase.co
# Service role key bypasses RLS — NEVER expose to the browser
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# ─── Optional: AI features ───────────────────────────────────────────────────
GEMINI_API_KEY=your_gemini_api_key_here
